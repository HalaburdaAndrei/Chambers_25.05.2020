    <fields>
        <fullName>Total_Price_Current_FY__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Opportunity.CloseDate &gt; DATE(YEAR(TODAY()) ,4,30),
IF( AND(Opportunity.CloseDate &gt; DATE(YEAR(TODAY()) ,4,30),Opportunity.CloseDate &lt; DATE(YEAR(TODAY())+1 ,5,1)) , TotalPrice ,
0),
0
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Price Current FY</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Total_Price_Prev_FY_TO_DATE__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Opportunity.CloseDate &lt; DATE(YEAR(TODAY()),4,30),
IF(TODAY() &gt; DATE(YEAR(TODAY()),4,30),
IF(AND(Opportunity.CloseDate &gt; DATE(YEAR(TODAY())-1,4,30),Opportunity.CloseDate &lt; DATE(YEAR(TODAY())-1,MONTH(TODAY()),DAY(TODAY())) ), TotalPrice ,0),
IF(AND(Opportunity.CloseDate &gt; DATE(YEAR(TODAY())-1,4,30),Opportunity.CloseDate &lt; DATE(YEAR(TODAY()),MONTH(TODAY()),DAY(TODAY())) ), TotalPrice ,0)),
0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Price Prev FY TO DATE</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Total_Price_Prev_FY__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Opportunity.CloseDate  &lt; DATE(YEAR(TODAY()) ,4,30),
IF( AND(Opportunity.CloseDate &gt; DATE(YEAR(TODAY())-1 ,4,30),Opportunity.CloseDate &lt; DATE(YEAR(TODAY()) ,5,1)) ,  TotalPrice ,0),
0
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Price Prev FY</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
